   "source": [
    "import numpy as np\n",
    "\n",
    "def read_bin_1d(filename):\n",
    "    return np.fromfile(filename, dtype=np.int32)\n",
    "\n",
    "\n",
    "def read_bin_2d(filename, shape):\n",
    "    arr = np.fromfile(filename, dtype=np.int32)\n",
    "    return arr.reshape(shape)\n"
